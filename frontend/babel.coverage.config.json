{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "defaults",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["babel-plugin-inferno", {"imports": true}],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "istanbul"
  ]
}
